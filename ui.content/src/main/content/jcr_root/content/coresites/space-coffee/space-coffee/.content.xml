                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="coresites/components/content/text"
                        text="&lt;h2 style=&quot;text-align: center;&quot;>Brew the perfect cup&lt;/h2>&#xa;&lt;h3 style=&quot;text-align: center;&quot;>With our patented Mercurial™ coffee machine&lt;/h3>&#xa;"
                        textIsRich="true"/>
                    <video
                        jcr:created="{Date}2018-04-30T17:15:44.932-07:00"
                        jcr:createdBy="admin"
                        jcr:lastModified="{Date}2018-04-30T17:17:00.904-07:00"
