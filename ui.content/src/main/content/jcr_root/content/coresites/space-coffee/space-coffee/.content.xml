                    </video>
                    <text_87771024
                        jcr:created="{Date}2018-04-30T17:17:25.145-07:00"
                        jcr:createdBy="admin"
                        jcr:lastModified="{Date}2018-04-30T17:26:59.526-07:00"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="coresites/components/content/text"
                        text="&lt;h4 style=&quot;text-align: center;&quot;>A coffee machine designed by rocket scientists&lt;/h4>&#xa;&lt;ul>&#xa;&lt;li>Vortex foaming nozzles aerate the coffee for unsurpassed smoothness&lt;/li>&#xa;&lt;li>Precise temperature control with thermocouple technology&lt;/li>&#xa;&lt;li>No-mess gravity brewing system&lt;/li>&#xa;&lt;li>Schedule Luna bean refill orders directly from the Mercurial touch interface&lt;/li>&#xa;&lt;/ul>&#xa;"
                        textIsRich="true">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                offset="3"
                                width="6"/>
